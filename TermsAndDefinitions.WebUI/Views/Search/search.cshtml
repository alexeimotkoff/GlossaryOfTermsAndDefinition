@using TermsAndDefinitions.WebUI.ViewModels;
@model SearchQuery
    
    @switch (Model.typeSearching)
    {
        case 0:
            Html.Action("GetTerms", "Search", Model);
            break;
        case 1:
            Html.Action("GetTermsByDefinition", "Search", Model);
            break;
        default:
            Html.Action("GetTerms", "Search", new { Model});
            Html.Action("GetTermsByDefinition", "Search", Model);
            break;
    }         
