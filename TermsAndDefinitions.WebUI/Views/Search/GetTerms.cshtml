@using System.Collections;
@using TermsAndDefinitions.WebUI.ViewModels;
@model IEnumerable<VTerm>

@if (Model.Count() > 0)
{
    <div>
        <h2>Поиск по термину</h2>
        @foreach (var item in Model)
        {
            <div class="well">
                <h4>
                    @item.TermName - @item.Description.Description 
                </h4>
            </div>
        }
    </div>
}
