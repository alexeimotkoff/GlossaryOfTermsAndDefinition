@using System.Collections;
@using TermsAndDefinitions.WebUI.ViewModels;
@model IEnumerable<VTerm>

@if (Model.Count() > 0)
{
    <div>
        <h2>Поиск по термину</h2>
        @foreach (var item in Model)
        {
            <h3>@item.TermName - @item.Description.Description </h3>
        }
    </div>
}
