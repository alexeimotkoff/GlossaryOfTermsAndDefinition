@{
    ViewBag.Title = "Index";
}
@using System.Collections;
@using TermsAndDefinitions.WebUI.ViewModels;
@model IEnumerable<VTerm>
<h2>Index</h2>
    @{ 
        if (Model != null)
        {
            foreach (var item in Model)
            {
            <h2>@item.TermName - @item.Description.Description </h2>
            }
        }
    }


